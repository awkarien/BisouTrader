{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": "dist",
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/frames/(.*)",
      "dest": "/api/frames.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}